{
    "watch": [
        "server.js",
        "app.js",
        "controllers",
        "routes",
        "middleware",
        "models",
        "services",
        "config",
        "utils"
    ],
    "ext": "js,json",
    "ignore": [
        "logs/**",
        "*.log",
        "node_modules/**",
        "docs/**",
        "training/**",
        "*.md",
        "package-lock.json"
    ],
    "delay": "200ms",
    "env": {
        "NODE_ENV": "development"
    }
}